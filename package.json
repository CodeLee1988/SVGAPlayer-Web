{
  "name": "svgaplayerweb",
  "version": "2.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/yyued/SVGAPlayer-Web.git"
  },
  "main": "./build/svga.min.js",
  "types": "index.d.ts",
  "scripts": {
    "dev:canvas": "node ./scripts/dev.js --engine canvas",
    "dev:createjs": "node ./scripts/dev.js --engine createjs",
    "dev": "node ./scripts/dev.js --engine all",
    "build:canvas": "webpack --config ./scripts/build.js --optimize-minimize --mode production --env.engine canvas",
    "build:createjs": "webpack --config ./scripts/build.js --optimize-minimize --mode production --env.engine createjs",
    "build": "npm run build:canvas && npm run build:createjs"
  },
  "author": "YYUED",
  "license": "Apache License 2.0",
  "dependencies": {
    "pako": "^1.0.6",
    "protobufjs": "^6.8.6"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "browser-sync": "^2.23.6",
    "chalk": "^2.3.2",
    "ts-loader": "^4.0.1",
    "typescript": "^2.7.2",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.11",
    "yargs": "^11.0.0"
  }
}
